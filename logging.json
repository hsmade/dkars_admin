{
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "%(asctime)s:%(levelname)s:%(name)s:%(message)s"
    }
  },
  "handlers": {
    "file_handler": {
      "backupCount": "20",
      "class": "logging.handlers.RotatingFileHandler",
      "encoding": "utf8",
      "filename": "dkars.log",
      "formatter": "simple",
      "level": "DEBUG",
      "maxBytes": "10485760"
    }
  },
  "root": {
    "handlers": [
      "file_handler"
    ],
    "level": "DEBUG"
  },
  "version": 1
}
